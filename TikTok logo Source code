from turtle import *
t = Turtle()
s = Screen()
s.bgcolor('black')
t.width(30)
t.color('#db0f3c')
t.left(180)
t.circle(50, 270)
t.forward(120)
t.left(180)
t.circle(50, 90)
t.color('#50ebe7')
t.up()
t.goto(-5, 13)
t.down()
t.left(180)
t.circle(50, 270)
t.forward(120)
t.left(180)
t.circle(50, 90)
t.color('white')
t.up()
t.goto(-5, 5)
t.down()
t.left(180)
t.circle(50, 270)
t.forward(120)
t.left(180)
t.circle(50, 90)
t.done()
